<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_param"
    app:startDestination="@id/parametersMainFragment">

    <fragment
        android:id="@+id/parametersMainFragment"
        android:name="com.example.appfall.views.fragments.ParametersMainFragment"
        android:label="fragment_parameters_main"
        tools:layout="@layout/fragment_parameters_main" >
        <action
            android:id="@+id/action_parametersMainFragment_to_nameFragment"
            app:destination="@id/nameFragment" />
        <action
            android:id="@+id/action_parametersMainFragment_to_passwordFragment"
            app:destination="@id/passwordFragment" />
    </fragment>
    <fragment
        android:id="@+id/nameFragment"
        android:name="com.example.appfall.views.fragments.NameFragment"
        android:label="fragment_name"
        tools:layout="@layout/fragment_name" >
        <action
            android:id="@+id/action_nameFragment_to_parametersMainFragment"
            app:destination="@id/parametersMainFragment" />
    </fragment>
    <fragment
        android:id="@+id/passwordFragment"
        android:name="com.example.appfall.views.fragments.PasswordFragment"
        android:label="fragment_password"
        tools:layout="@layout/fragment_password" >
        <action
            android:id="@+id/action_passwordFragment_to_parametersMainFragment"
            app:destination="@id/parametersMainFragment" />
    </fragment>

    <action
        android:id="@+id/action_global_parametersMainFragment"
        app:destination="@id/parametersMainFragment" />
</navigation>